      "request": "launch",
      "name": "Debug unit tests in library 'duniter'",
      "cargo": {
        "args": ["test", "--no-run", "--lib", "--package=duniter"],
        "filter": {
          "name": "duniter",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
